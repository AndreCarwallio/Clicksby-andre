<style>

/* Editing card icons (top-right) */
.editCard { position: relative; }
.editAppIcon{
  position:absolute;
  top:18px;
  right:18px;
  width:44px;
  height:44px;
  border-radius:14px;
  overflow:hidden;
  box-shadow: 0 10px 25px rgba(0,0,0,.25);
}
.editAppIcon svg{ width:100%; height:100%; display:block; }



/* --- Brand blue for tool names --- */
.toolBlue{ color:#31A8FF; font-weight:800; }

/* --- Icon badge (top-right) --- */
.editCard{ position:relative; }
.editAppIcon{
  position:absolute;
  top:18px;
  right:18px;
  width:52px;
  height:52px;
  border-radius:16px;
  overflow:hidden;
  box-shadow: 0 12px 28px rgba(0,0,0,.28);
  background: rgba(255,255,255,.06);
  display:flex;
  align-items:center;
  justify-content:center;
}
.editAppIcon svg{ width:100%; height:100%; display:block; }

/* --- Make the Editing cards feel more premium --- */
.editCard{
  border: 1px solid rgba(255,255,255,.12);
  box-shadow: 0 24px 70px rgba(0,0,0,.35);
}
.editCard::before{
  content:"";
  position:absolute;
  inset:-2px;
  border-radius: 22px;
  background: linear-gradient(135deg, rgba(49,168,255,.55), rgba(34,197,94,.35), rgba(124,58,237,.45));
  z-index:-1;
  filter: blur(10px);
  opacity: .65;
}
.editCard ul li::marker{ color:#31A8FF; }

/* --- How it works: brighter + more attractive --- */
#how, .howSection, .howItWorks, .howWrap{ position:relative; }

.howCard, .howStep, .howStepCard, .howItem{
  background: linear-gradient(180deg, #15171b, #0f1114) !important;
  border: 1px solid rgba(49,168,255,.20) !important;
  box-shadow: 0 24px 60px rgba(0,0,0,.35) !important;
}

.howCard::before, .howStep::before, .howStepCard::before, .howItem::before{
  content:"";
  position:absolute;
  inset:-2px;
  border-radius: 22px;
  background: linear-gradient(135deg, rgba(49,168,255,.55), rgba(124,58,237,.35), rgba(34,197,94,.25));
  z-index:-1;
  filter: blur(12px);
  opacity: .55;
}

.howIcon, .howStepIcon, .stepIcon, .iconBubble{
  background: linear-gradient(135deg, rgba(49,168,255,.22), rgba(124,58,237,.18)) !important;
  border: 1px solid rgba(49,168,255,.28) !important;
  box-shadow: 0 14px 30px rgba(49,168,255,.18) !important;
}

.howArrow, .arrow, .stepArrow, .howSteps .arrowIcon{
  color:#31A8FF !important;
  filter: drop-shadow(0 8px 14px rgba(49,168,255,.35));
}



/* Force official PNG icons in the top-right */
.editCard{ position:relative; }
.editAppIcon{
  position:absolute;
  top:18px;
  right:18px;
  width:56px;
  height:56px;
  border-radius:16px;
  overflow:hidden;
  box-shadow: 0 14px 30px rgba(0,0,0,.28);
  background: rgba(255,255,255,.10);
  border: 1px solid rgba(255,255,255,.14);
  display:flex;
  align-items:center;
  justify-content:center;
}
.editAppIcon__img{
  width:72%;
  height:72%;
  object-fit:contain;
  display:block;
  filter: drop-shadow(0 8px 14px rgba(0,0,0,.35));
}

